- data = data || {}
- var pageTitle = data.title || "sosaics"

doctype html
html
  head
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible",content="IE=edge")
    meta(name="viewport",content="width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=no")

    link(rel="icon",href="/public/images/favicon.png")
    link(rel='stylesheet',type='text/css',href='/public/css/mosaicjs-master.css')
    script(src='/public/js/dependencies/angular.min.js')
    script(src='/public/js/dependencies/uploader.min.js')
    script(src='/public/js/dependencies/angulargrid.min.js')
    script(src='/public/js/app.min.js')
    
    script(type='text/javascript').
      LOCAL_DATA=!{JSON.stringify(data.external||{})}

    title= pageTitle
  body(data-ng-app="MosaicJS",data-ng-controller="GlobalController")
    div.container-fluid
      include header.jade
    div.container-fluid

      - //main entry point for any views that use layout as its primary page
      block main
